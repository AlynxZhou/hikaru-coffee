<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in posts %}
  <url>
    <loc>{{ getUrl(post.docPath) }}</loc>
    {% if post.date %}
    <lastmod>{{ moment(post.date).format("YYYY-MM-DD HH:mm:ss") }}</lastmod>
    {% endif %}
  </url>
  {% endfor %}
</urlset>
